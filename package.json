{
  "name": "vue-schema-form",
  "version": "0.1.0",
  "description": "Generate and validate forms from a JSON schema in a breeze",
  "main": "src/components/SchemaForm.vue",
  "scripts": {
    "lint": "eslint -c node_modules/linters/.eslintrc.js --ext .js,.vue --max-warnings=0 src test",
    "test": "jest",
    "prepush": "npm run lint"
  },
  "dependencies": {
    "map-to-object": "^0.1.0"
  },
  "devDependencies": {
    "babel-jest": "^22.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.18.2",
    "eslint-plugin-vue": "^4.3.0",
    "husky": "^0.14.3",
    "hyperform": "^0.9.9",
    "jest": "^22.4.0",
    "linters": "^0.1.0",
    "vue": "^2.5.13",
    "vue-jest": "^2.1.0",
    "vue-template-compiler": "^2.5.13"
  },
  "author": "Tom Panier <tpanier@synbioz.com>",
  "license": "UNLICENSED",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "setupFiles": [
      "./test/setup.js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/?!(map-to-object)"
    ]
  }
}
