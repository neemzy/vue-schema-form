{
  "name": "vue-schema-form",
  "version": "0.2.0",
  "description": "Generate and validate forms from a JSON schema in a breeze",
  "main": "src/components/SchemaForm.vue",
  "author": "neemzy <tom.panier@free.fr>",
  "license": "UNLICENSED",
  "scripts": {
    "test": "jest"
  },
  "gitHooks": {
    "pre-push": "npm test"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/preset-env": "^7.3.1",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.4.0",
    "hyperform": "^0.9.9",
    "jest": "^22.4.0",
    "vue": "^2.5.13",
    "vue-jest": "^2.1.0",
    "vue-template-compiler": "^2.5.13",
    "yorkie": "^2.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "setupFiles": [
      "./test/setup.js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/?!(map-to-object)"
    ],
    "testURL": "http://localhost/"
  }
}
